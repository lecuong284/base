{
    "name": "lecuong/base",
    "type": "library",
    "description": "Laravel Lecuong's base package, which offers admin authentication and a blank admin panel using AdminLTE",
    "keywords": [],
    "homepage": "https://github.com/lecuong284/base",
    "license": "MIT",
    "authors": [
        {
            "name": "Le Van Cuong",
            "email": "lecuong732@gmail.com",
            "homepage": "http://www.lecuong732.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "almasaeed2010/AdminLTE" : "2.3.*",
        "laravel/framework": "5.3.*|5.4.*",
        "prologue/alerts": "^0.4.1",
        "jenssegers/date": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "lecuong/generators": "^1.1",
        "laracasts/generators": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Lecuong\\Base\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lecuong\\Base\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
